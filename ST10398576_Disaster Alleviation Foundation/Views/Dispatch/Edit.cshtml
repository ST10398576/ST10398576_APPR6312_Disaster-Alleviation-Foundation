@model ST10398576_Disaster_Alleviation_Foundation.Models.Dispatch

<h2>Create Dispatch</h2>

<form asp-action="Edit" method="post">
    <div class="form-group">
        <label asp-for="DisasterIncidentID"></label>
        <select asp-for="DisasterIncidentID" asp-items="ViewBag.DisasterIncidentID" class="form-control"></select>
    </div>

    <div class="form-group">
        <label asp-for="ResourceDonationID"></label>
        <select asp-for="ResourceDonationID" asp-items="ViewBag.ResourceDonationID" class="form-control"></select>
    </div>

    <div class="form-group">
        <label asp-for="QuantityDispatched"></label>
        <input asp-for="QuantityDispatched" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Notes"></label>
        <textarea asp-for="Notes" class="form-control"></textarea>
    </div>

    <button type="submit" class="btn btn-success">Save</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
