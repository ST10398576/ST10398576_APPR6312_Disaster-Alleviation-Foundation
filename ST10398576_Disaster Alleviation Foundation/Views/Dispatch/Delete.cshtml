@model ST10398576_Disaster_Alleviation_Foundation.Models.Dispatch

<h2>Delete Dispatch</h2>

<p>Are you sure you want to delete this dispatch?</p>

<dl class="row">
    <dt class="col-sm-2">Incident</dt>
    <dd class="col-sm-10">@Model.DisasterIncident?.DisasterIncidentType</dd>

    <dt class="col-sm-2">Resource</dt>
    <dd class="col-sm-10">@Model.Resource?.ResourceDonationType</dd>

    <dt class="col-sm-2">Quantity</dt>
    <dd class="col-sm-10">@Model.QuantityDispatched</dd>
</dl>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" asp-for="DispatchID" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
