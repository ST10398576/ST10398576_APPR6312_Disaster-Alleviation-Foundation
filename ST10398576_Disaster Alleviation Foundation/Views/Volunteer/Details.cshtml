@model ST10398576_Disaster_Alleviation_Foundation.Models.Volunteer

<h2>Volunteer Details</h2>

<div>
    <strong>Name:</strong> @Model.User?.FullName <br />
    <strong>Email:</strong> @Model.User?.Email <br />
    <strong>Skills:</strong> @Model.Skills <br />
    <strong>Availability:</strong> @Model.Availability
</div>

<p>
    <a asp-action="Edit" asp-route-id="@Model.VolunteerID" class="btn btn-warning">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
</p>